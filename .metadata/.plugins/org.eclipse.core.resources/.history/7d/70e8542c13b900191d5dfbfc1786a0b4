package practice;
import java.util.*;
public class Sample {
public static void main(String[] args) {
	Scanner sc= new Scanner(System.in);
System.out.println("enter the highest power of 2");
int n=sc.nextInt();
for(int i=0;i<=n;i++) {
	int p=pow(2,i);
	System.out.println(p);
}

}

public static int pow(int k,int p )
{
	int pt=1;
	while(p>0) 
	{
		pt=pt*k;
		p--;
	}
	return pt;
}
}

