package practice;

import java.io.BufferedReader;
import java.io.File;
import java.io.FileNotFoundException;
import java.io.FileReader;
import java.util.Scanner;

public class MyList {
public class Node
{
	int data;
	Node next;
	public Node(int data) 
	{
		this.data=data;
	}
}
	Node head;
	
	public boolean addFirst(String s)
	{  int num=Integer.parseInt(s);
		Node n= new Node(num);
		if(head==null) {
			head=n;
			return true;
		}
		else
			n.next=head;
		return true;
	
	}
	public void sort() {
		Node t=head;
		while(t!=null) 
		{
			if(t.data>t.next.data) 
			{
				int f=t.data;
				t.data=t.next.data;
				t.next.data=f;
			}
			t=t.next;
	}
	}
		
		public void print() 
		{ 
			Node t=head;
		  System.out.println(t.data + " ");
		  t=t.next;
		}
		


	public static void main(String[] args) 
	{   String s[]=null;
		MyList ml= new MyList();
	
		int i=0;
		try {
		    File file=new File("/home/admin1/Documents/Sample.txt"); 
		    	    Scanner sc = new Scanner(file); 
		    	  
		    	    while (sc.hasNextLine()) 
		    	      s=sc.nextLine().split(" "); 
		    	      for(String str:s) {
		    	    	  ml.addFirst(str);
		    	      }
			}
		
		catch(Exception e) 
		{
	
			e.printStackTrace();
		}
		ml.sort();
		System.out.println("the sorted list is");
		   ml.print();
	
	}
}

