package practice;

public class Sample {
public static void main(String[] args) {
	int ar[]= {-2,2,0,4,-5,6,-1};
	int n=ar.length;
	for(int i=0;i<n-2;i++) {
		for(int j=i+1;j< n-1;j++) {
			for(int k=j+1;k<n;k++) {
				if(ar[i]+ar[j]+ar[k]==0) {
					System.out.println(ar[i]+" "+ar[j]+" "+ar[k]);
				}
			}
		}
	}
}
}
