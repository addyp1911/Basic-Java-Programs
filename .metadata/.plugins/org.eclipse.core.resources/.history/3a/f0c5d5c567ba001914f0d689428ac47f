package practice;

import java.io.File;
import java.io.FileNotFoundException;
import java.io.FileReader;
import java.util.Scanner;

public class MyList {
public class Node
{
	int data;
	Node next;
	public Node(int data) 
	{
		this.data=data;
	}
}
	Node head;
	
	public boolean addFirst(int num)
	{  
		Node n= new Node(num);
		if(head==null) {
			head=n;
			return true;
		}
		else {
			n.next=head;
			head=n;
		}
		return true;
	
	}
	public void sort() 
	{
		Node t=head;
		while(t!=null) 
		{
			if(t.data>t.next.data) 
			{
				int f=t.data;
				t.data=t.next.data;
				t.next.data=f;
			}
			t=t.next;
	}
	}
		
		public void print() 
		{ 
			Node t=head;
		  System.out.println(t.data + " ");
		  t=t.next;
		}
		


	public static void main(String[] args) 
	{   int s=0;
		MyList ml= new MyList();
	
		try {
		    File file=new File("/home/admin1/Documents/number.odt"); 
		    	    Scanner sc = new Scanner(file); 
		    	  
		    	    while (sc.hasNext())
		    	    {    int i=sc.nextInt();
		    	         ml.addFirst(i);
		    	     }
		    	 
			}
		
		catch(Exception e) 
		{
	
			e.printStackTrace();
		}
		ml.sort();
		System.out.println("the sorted list is");
		   ml.print();
	
	}
}

