package practice;
import java.util.*;
public class Sample {
public static void main(String[] args)
{  Scanner sc= new Scanner(System.in);
	String ar[]= {"cat","dog","mat","rat"};
	System.out.println("enter the string you want to search");
	String s=sc.next();
	int l=0;
	int r=ar.length-1;
	int m=l+r/2;
	while(l<=r) 
	{
		if(s.equals(ar[m]))
			System.out.println("found");
		
		else
			if(s.compareToIgnoreCase(ar[m])>0) {
				l=m|+1;
			}
			else 
				r=m-1;
	}
}

}